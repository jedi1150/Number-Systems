<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LightTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textColorPrimaryInverse">#202125</item>
        <item name="android:textColorPrimary">#202125</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorPrimaryDark">#fff</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorSecondary">#fff</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">#FFF</item>
        <item name="android:navigationBarDividerColor">#FFF</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="iconColor">#202125</item>
        <item name="iconColorDisabled">#757575</item>
        <item name="tabSelected">@color/colorPrimary</item>
        <item name="android:fontFamily">@font/open_sans_semibold</item>
    </style>

    <style name="DarkTheme" parent="Theme.MaterialComponents.NoActionBar">
        <item name="android:textColorPrimaryInverse">#bdbdbd</item>
        <item name="android:textColorPrimary">#bdbdbd</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorPrimaryDark">#202125</item>
        <item name="colorPrimary">#bdbdbd</item>
        <item name="colorSecondary">#202125</item>
        <item name="android:color">@color/colorPrimary</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">#202125</item>
        <item name="android:navigationBarDividerColor">#202125</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="iconColor">#bdbdbd</item>
        <item name="iconColorDisabled">#757575</item>
        <item name="tabSelected">@color/colorPrimary</item>
        <item name="android:fontFamily">@font/open_sans_semibold</item>
    </style>


</resources>